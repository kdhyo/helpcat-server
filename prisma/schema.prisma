generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id                 Int      @default(autoincrement()) @id
  email              String?  @unique
  emailvalidated     Boolean? @default(false)
  validateEmailToken String?  @unique
  password           String
  userName           String
  nickName           String?  @unique
  gender             String
  phone              String
  address            String
  birh               String
  createdAt          DateTime @default(now())
}
